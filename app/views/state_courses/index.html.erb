<p id="notice"><%= notice %></p>

<h1>Listing State Courses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_courses.each do |state_course| %>
      <tr>
        <td><%= state_course.name %></td>
        <td><%= link_to 'Show', state_course %></td>
        <td><%= link_to 'Edit', edit_state_course_path(state_course) %></td>
        <td><%= link_to 'Destroy', state_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State course', new_state_course_path %>
